                                    "display": "${wifiButtonDisplay}",
                                    "spacing": "@spacing2XSmall",
                                    "items": [
                                        {
                                            "type": "AlexaIconButton",
                                            "id": "WIFI_INFO_BUTTON",
                                            "vectorSource": "M23.6 6C17-.1 6.7-.1 0 6c-.2.2-.3.6 0 .9l1.3 1.3c.2.2.6.2.8 0 5.5-5 13.9-5 19.4 0 .2.2.6.2.8 0l1.3-1.3c.2-.2.2-.6 0-.9zm-11.8 7.4c-1.3 0-2.4 1.1-2.4 2.4s1.1 2.4 2.4 2.4 2.4-1.1 2.4-2.4-1.1-2.4-2.4-2.4zm7.6-3.1c-4.3-3.9-10.9-3.8-15.2 0-.2.2-.2.6 0 .8l1.3 1.3c.2.2.6.2.8 0 3.1-2.7 7.9-2.7 11 0 .2.2.6.2.8 0l1.3-1.3c.2-.2.2-.6 0-.9z",
                                            "buttonSize": "@footerButtonSize",
                                            "buttonStyle": "contained",
                                            "primaryAction": {
                                                "type": "SendEvent",
                                                "componentId": "WIFI_INFO_BUTTON",
                                                "arguments": [
                                                    "WifiAccessFreeIntent"
                                                ],
                                                "flags": {
                                                    "interactionMode": "STANDARD"
                                                }
                                            }
                                        }        
                                    ]
                                },
                                {
