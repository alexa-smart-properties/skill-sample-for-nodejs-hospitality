    "layouts": {
        "hotelHeader": {
            "parameters": [
                {
                    "name": "clockDisplay",
                    "type": "string",
                    "default": "normal"
                },
                {
                  "name": "headerPrimaryText",
                  "type": "string",                  
                  "default": ""
                },
                {
                  "name": "headerSecondaryText",
                  "type": "string",                  
                  "default": ""
                },
                {
                    "name": "dividerImageSource",
                    "type": "string",                    
                    "default": ""
                },
                {
                    "name": "logoUrl",
                    "type": "string",                    
                    "default": ""
                }
            ],
            "items": [
                {
                    "direction": "row",
                    "alignItems": "center",
                    "type": "Container",
                    "height": "15%",
                    "width": "100%",   
                    "id": "hotelHeader", 
                    "items": [
                        {
                            "display": "${clockDisplay}",
                            "fontSize": "@headerTimeFontSize",
                            "color": "#FFFFFF",
                            "fontStyle": "normal",
                            "textAlign": "left",
                            "textAlignVertical": "center",
                            "text": "${Time.format('HH:mm', T)}",
                            "type": "Text",
                            "bind": [
                                {
                                    "name": "T",
                                    "value": "${localTime}"
                                },
                                {
                                    "name": "H",
                                    "value": "${Time.hours(localTime)}"
                                }
                            ],
                            "width": "30%",
                            "paddingLeft": "@headerPaddingLeft"
                        },
                        {
                            "direction": "column",
                            "alignItems": "stretch",
                            "justifyContent": "center",
                            "items": [
                                {
                                    "fontSize": "@headerGuestNameFontSize",
                                    "fontFamily": "Amazon Ember Display",
                                    "fontStyle": "normal",
                                    "color": "#FFFFFF",
                                    "textAlign": "right",
                                    "textAlignVertical": "center",
                                    "text": "${headerPrimaryText}",
                                    "type": "Text",
                                    "paddingTop": "13dp"
                                },
                                {
                                    "fontSize": "@headerRoomFontSize",
                                    "fontFamily": "Amazon Ember Display",
                                    "fontStyle": "normal",
                                    "color": "#FFFFFF",
                                    "textAlign": "right",
                                    "textAlignVertical": "center",
                                    "text": "${headerSecondaryText}",
                                    "type": "Text"
                                }
                            ],
                            "type": "Container",
                            "width": "45%"
                        },
                        {
                            "source": "${dividerImageSource}",
                            "type": "Image",
                            "height": "@headerDividerHeight",
                            "width": "@headerDividerWidth",
                            "paddingTop": "13",
                            "alignSelf": "center"
                        },
                        {
                            "align": "left",
                            "source": "${logoUrl}",
                            "scale": "best-fit-down",
                            "type": "Image",
                            "height": "@headerLogoHeight",
                            "width": "@headerLogoHeight",
                            "paddingRight": "@headerPaddingRight",
                            "paddingTop": "13"
                        }
                    ]
                }   
            ]
        },
        "hotelFooter": {
            "parameters": [
                {
                    "name": "hintTextArray",
                    "type": "array",
                    "default": []
                },
                {
                    "name": "requstSubmitButtonDisplay",
                    "type": "string",
                    "default": "none"
