                                            "when": "${(@viewportProfile == @hubLandscapeXLarge)}",
                                            "type": "AlexaButton",
                                            "id": "SUBMIT_BUTTON",
                                            "buttonStyle": "contained",
                                            "disabled": "${amenitiesTotal == 0}",
                                            "buttonText": "Send Request",
                                            "transform": [ { "scaleX": 1.5 }, { "scaleY": 1.5 }, {"translateX": "-20%"}],
                                            "primaryAction": [
                                                {
                                                    "type": "SendEvent",
                                                    "arguments": [
                                                        "${amenitiesAmounts}"
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "when": "${(@viewportProfile != @hubLandscapeXLarge)}",
                                            "type": "AlexaButton",
                                            "id": "SUBMIT_BUTTON",
                                            "buttonStyle": "contained",
                                            "disabled": "${amenitiesTotal == 0}",
                                            "buttonText": "Send Request",
                                            "primaryAction": [
                                                {
                                                    "type": "SendEvent",
                                                    "arguments": [
                                                        "${amenitiesAmounts}"
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
